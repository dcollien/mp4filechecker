{
  "name": "mp4filechecker",
  "version": "1.0.2",
  "description": "Check details of an mp4 file, e.g. to determine if transcoding is required",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "bundle-example": "cd example && rollup -c"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "video",
    "mp4",
    "avc1",
    "transcode",
    "codec",
    "compression",
    "upload"
  ],
  "author": "David Collien",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/dcollien/mp4filechecker.git"
  },
  "license": "MIT",
  "devDependencies": {
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "mp4box": "github:dcollien/mp4box.js#fixes"
  }
}
